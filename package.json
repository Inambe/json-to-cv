{
	"name": "make-resume",
	"version": "1.0.0",
	"description": "Separate data from design and build beautiful Resumes.",
	"main": "./lib/index.js",
	"scripts": {
		"test": "jest",
		"test:coverage": "npm test -- --coverage",
		"postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
	},
	"bin": "./bin/index.js",
	"keywords": [
		"resume",
		"resume-builder",
		"cv",
		"cv-builder"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/make-resume/make-resume"
	},
	"homepage": "https://github.com/make-resume/make-resume",
	"author": "Inam Ul Huq",
	"license": "MIT",
	"dependencies": {
		"chalk": "^4.0.0",
		"chokidar": "^3.4.0",
		"cli-table": "^0.3.1",
		"commander": "^5.1.0",
		"handlebars": "^4.7.6",
		"make-resume-base": "^1.0.0",
		"ncp": "^2.0.0",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2"
	},
	"devDependencies": {
		"@types/jest": "^26.0.0",
		"jest": "^26.0.1"
	}
